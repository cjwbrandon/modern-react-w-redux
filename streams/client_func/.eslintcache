","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"40"},{"filePath":"71","messages":"72","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"40"},{"filePath":"73","messages":"74","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"75","messages":"76","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/index.js",[],["77","78"],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/App.js",["79"],"import React from \"react\";\nimport { Router, Route, Switch } from \"react-router-dom\";\n\nimport history from \"../history\";\nimport Header from \"./Header\";\nimport StreamOverview from \"./streams/StreamOverview\";\nimport StreamCreate from \"./streams/StreamCreate\";\nimport StreamDelete from \"./streams/StreamDelete\";\nimport StreamEdit from \"./streams/StreamEdit\";\nimport StreamShow from \"./streams/StreamShow\";\n\nconst App = () => {\n  return (\n    <div className=\"ui container\">\n      <Router history={history}>\n        <div>\n          <Header />\n          <Switch>\n            <Route path=\"/\" exact component={StreamOverview} />\n            <Route path=\"/streams/create\" exact component={StreamCreate} />\n            <Route path=\"/streams/delete/:id\" exact component={StreamDelete} />\n            <Route path=\"/streams/edit\" exact component={StreamEdit} />\n          </Switch>\n        </div>\n      </Router>\n    </div>\n  );\n};\n\nexport default App;\n","/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamCreate.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamEdit.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamOverview.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamShow.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamDelete.js",["80"],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/history.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/Header.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/GoogleAuth.js",["81"],"import React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport { signIn, signOut } from \"../actions\";\n\nconst GoogleAuth = (props) => {\n  let auth;\n\n  useEffect(() => {\n    // Load auth client\n    window.gapi.load(\"client:auth2\", () => {\n      // Initialise client with client id and scope\n      window.gapi.client\n        .init({\n          clientId: process.env.REACT_APP_CLIENT_ID,\n          scope: \"email\",\n        })\n        .then(() => {\n          auth = window.gapi.auth2.getAuthInstance();\n          onAuthChange(auth.isSignedIn.get());\n          auth.isSignedIn.listen(onAuthChange);\n        });\n    });\n  }, []);\n\n  const onAuthChange = (isSignedIn) => {\n    if (isSignedIn) props.signIn(auth.currentUser.get().getId());\n    else props.signOut();\n  };\n\n  const onSignInClick = () => {\n    auth.signIn();\n  };\n\n  const onSignOutClick = () => {\n    auth.signOut();\n  };\n\n  const renderAuthButton = () => {\n    if (props.isSignedIn === null) return null;\n    if (props.isSignedIn)\n      return (\n        <button className=\"ui red google button\" onClick={onSignOutClick}>\n          <i className=\"google icon\" />\n          Sign Out\n        </button>\n      );\n    return (\n      <button className=\"ui red google button\" onClick={onSignInClick}>\n        <i className=\"google icon\" />\n        Sign In with Google\n      </button>\n    );\n  };\n\n  return <div>{renderAuthButton()}</div>;\n};\n\nconst mapStateToProps = (state) => {\n  return { isSignedIn: state.auth.isSignedIn };\n};\n\nexport default connect(mapStateToProps, { signIn, signOut })(GoogleAuth);\n","/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/actions/index.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/actions/types.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/reducers/index.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/reducers/authReducer.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/apis/streams.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/streams/StreamForm.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/reducers/streamsReducer.js",[],"/home/brandon/Desktop/Data Science/Courses/JavaScript/Modern React with Redux/modern-react-w-redux/streams/client_func/src/components/Modal.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","l